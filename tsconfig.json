{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "removeComments": false,
    "noImplicitAny": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "allowJs": false,
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules"
  ]
}